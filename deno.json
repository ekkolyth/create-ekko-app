{
  "name": "@mikekenway/create-ekko-app",
  "version": "1.0.16",
  "description": "Custom Next.js starter CLI with Shadcn, Resend, Bun, and more.",
  "exports": "./cli.ts",
  "tasks": {
    "dev": "deno run --allow-all cli.ts",
    "build": "deno compile --allow-all --output ./build/create-ekko-app cli.ts"
  },
  "imports": {
    "@cliffy/prompt": "jsr:@cliffy/prompt@^1.0.0-rc.5",
    "@std/path": "jsr:@std/path@^1.0.8"
  },
  "publish": {
    "exclude": [
      "build/",
      "*.tgz",
      "node_modules/"
    ]
  }
}
