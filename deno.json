{
  "name": "@mikekenway/create-ekko-app",
  "version": "1.1.30",
  "description": "Custom React Framework Starter ",
  "license": "MIT",
  "exports": "./cli.ts",
  "nodeModulesDir": "auto",
  "tasks": {
    "dev": "deno run --allow-all cli.ts",
    "build": "deno compile --allow-all --output ./build/create-ekko-app cli.ts",
    "build:npm": "deno run --allow-all scripts/build.ts",
    "sync-package": "deno run --allow-read --allow-write --no-check scripts/sync-package.ts",
    "publish": "deno run --allow-all scripts/publish.ts"
  },
  "imports": {
    "@cliffy/command": "jsr:@cliffy/command@^1.0.0-rc.8",
    "@cliffy/prompt": "jsr:@cliffy/prompt@^1.0.0-rc.5",
    "@std/path": "jsr:@std/path@^1.0.8",
    "@std/cli": "jsr:@std/cli@^1.0.6"
  },
  "publish": {
    "exclude": [
      "build/",
      "*.tgz",
      "node_modules/"
    ]
  }
}